{
  "listeners": {
    "*:80": {
      "pass": "routes"
    }
  },
  "routes": [
    {
      "match": {
        "host": "{{ project_address }}",
        "uri": "*/static/*"
      },
      "action": {
        "share": "/var/www/"
      }
    },
    {
      "match": {
        "host": "{{ project_address }}"
      },
      "action": {
        "proxy": "http://server:{{ server_port }}"
      }
    }
  ],
  "applications": {}
}
